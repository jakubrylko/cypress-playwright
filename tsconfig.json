{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6",
    "types": ["cypress-plugin-api", "node"]
  },
  "include": ["common", "cypress", "playwright", "types", "**/*.config.ts"]
}
